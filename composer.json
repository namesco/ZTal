{
	"name": "namesco/ztal",
	"homepage": "http://opensource.names.co.uk/ztal/",
	"description": "ZTal makes integrating the PHP templating system PHPTAL into Zend Framework easy.",
	"license": "Apache-2.0",
	"type": "library",
	"keywords": [
		"zope",
		"template engine",
		"phptal",
		"zend"
	],
	"minimum-stability": "dev",
	"require": {
		"php": ">=5.3.0",
		"phptal/phptal": "1.3.0"
	},
	"autoload": {
		"psr-0" : {
			"Ztal_" : ""
		}
	},
	"target-dir" : "Ztal",
    "include-path": [
        "/"
    ]
}